CONFIG_PACKAGE_luci-app-advanced=y
CONFIG_PACKAGE_luci-app-control-timewol=y
CONFIG_PACKAGE_luci-app-control-weburl=n
CONFIG_PACKAGE_luci-app-easyupdate=n
CONFIG_PACKAGE_luci-app-msd_lite=y
